--newsletterReminderPadding: 40px;
    --typeHeaderPrimary: ivypresto-display;
    --typeHeaderFallback: Cormorant;
  --typeHeaderSpacing: 1.5px;
    --typeBasePrimary: nitti-grotesk;
    --typeBaseFallback: Roboto;
    --typeBaseWeight: 300;
}
.index-section {
    margin: 0;
}

/* BODY and PARAGRAPH */

body, p, .grid-product__price, .cart__item-row small, input, select, textarea {
font-family: nitti-grotesk,sans-serif;
font-weight: 300;
font-style: normal;
letter-spacing: 0;
