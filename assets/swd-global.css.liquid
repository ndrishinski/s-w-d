/* ROOT */

:root {
--colorBtnPrimaryLight: #d1cabd;
--colorBtnPrimaryDim: #e0dacd;
--colorBodyDim: #eae6dd;
--colorDrawersDim: #eae6dd;
--colorDrawerTextDark: #26241e;
